<?xml version='1.0' encoding='utf-8'?>
<widget id="co.uk.gauntface.mobile.devicelab" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Hello Cordova</name>
    <preference name="loglevel" value="DEBUG" />
    <feature name="App">
        <param name="android-package" value="org.apache.cordova.App" />
    </feature>
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="CordovaGPlusOauthPlugin">
        <param name="android-package" value="co.uk.gauntface.cordova.plugin.nativegplussignin.CordovaGPlusOauthPlugin" />
    </feature>
    <feature name="GCMBrowserLaunch">
        <param name="android-package" value="co.uk.gauntface.cordova.plugin.gcmbrowserlaunch.GCMBrowserLaunch" />
    </feature>
    <name>Device Lab</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
</widget>
